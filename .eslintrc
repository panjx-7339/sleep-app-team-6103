{
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-native/all"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "react-native"
    ],
    "rules": {
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }